  border: 1px solid #eee;
}
.buy-num-box .num-box .num-input{
  font-size: 28rpx;
}
.buy-num-box .num-box .num-input input{
  height: 100%;
}
.confirm{
  width: 100%;
  height: 89rpx;
  text-align: center;
  line-height: 89rpx;
  font-size: 30rpx;
  color:#ffffff;
  background-color: #EB4D50;
}
.outof {
  background-color: #555;
}
.buy-num-box .num-box .hui{
  background-color: #f5f5f9;
}
.curKanjiaprogress {
